{
  "version": 2,
  "name": "intelligestor-backend-rlyo",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "False"
  }
}
