{
  "version": 2,
  "name": "intelligestor-backend-rlyo",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "False"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
